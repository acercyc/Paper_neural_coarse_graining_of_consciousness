 @article{Mudrik_Faivre_Koch,
 	 title={Information integration without awareness}, ISSN={1364-6613}, url={http://www.sciencedirect.com/science/article/pii/S1364661314001041}, DOI={10.1016/j.tics.2014.04.009}, abstractNote={Information integration and consciousness are closely related, if not interdependent. But, what exactly is the nature of their relation? Which forms of integration require consciousness? Here, we examine the recent experimental literature with respect to perceptual and cognitive integration of spatiotemporal, multisensory, semantic, and novel information. We suggest that, whereas some integrative processes can occur without awareness, their scope is limited to smaller integration windows, to simpler associations, or to ones that were previously acquired consciously. This challenges previous claims that consciousness of some content is necessary for its integration; yet it also suggests that consciousness holds an enabling role in establishing integrative mechanisms that can later operate unconsciously, and in allowing wider-range integration, over bigger semantic, spatiotemporal, and sensory integration windows.}, journal={Trends in Cognitive Sciences}, author={Mudrik, Liad and Faivre, Nathan and Koch, Christof}}
